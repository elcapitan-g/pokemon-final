{
  "openapi": "3.0.0",
  "info": {
    "title": "Pokémon Card Collection API",
    "version": "1.0.0",
    "description": "API for managing Pokémon cards and master set prices."
  },
  "tags": [
    {
      "name": "Your Pokemon Cards",
      "description": "Manage your personal Pokémon card collection."
    },
    {
      "name": "Master Sets",
      "description": "Manage and view master set price data."
    },
    {
      "name": "Chase Cards",
      "description": "Manage and view your Dream cards."
    },
    {
      "name": "ETB",
      "description": "Manage and view your sealed Elite Trainer Boxes."
    }
  ],
  "paths": {
    "/cards": {
      "get": {
        "tags": ["Your Pokemon Cards"],
        "summary": "Get all of your Pokémon cards",
        "responses": {
          "200": {
            "description": "List of your Pokémon cards"
          }
        }
      },
      "post": {
        "tags": ["Your Pokemon Cards"],
        "summary": "Add a new Pokémon card to your collection",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": { "type": "string", "example": "Pikachu" },
                  "setName": { "type": "string", "example": "Base Set" },
                  "rarity": { "type": "string", "example": "Common" },
                  "price": { "type": "number", "example": 12.5 },
                  "release_year": { "type": "integer", "example": 1999 }
                },
                "required": ["name", "setName", "rarity", "price"]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Card created successfully"
          },
            "401": {
    "description": "You must be logged in to perform this action."
  }
        }
      }
    },
    "/cards/{id}": {
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "description": "ID of the Pokémon card",
          "schema": { "type": "string" }
        }
      ],
      "get": {
        "tags": ["Your Pokemon Cards"],
        "summary": "Get a Pokémon card by ID",
        "responses": {
          "200": { "description": "Pokémon card found" },
          "404": { "description": "Card not found" }
        }
      },
      "put": {
        "tags": ["Your Pokemon Cards"],
        "summary": "Update one of your Pokémon card",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": { "type": "string", "example": "Pikachu" },
                  "setName": { "type": "string", "example": "Base Set" },
                  "rarity": { "type": "string", "example": "Common" },
                  "price": { "type": "number", "example": 12.5 },
                  "release_year": { "type": "integer", "example": 1999 }
                },
                "required": ["name", "setName", "rarity", "price"]
              }
            }
          }
        },
        "responses": {
          "200": { "description": "Card updated" },
            "401": {
    "description": "You must be logged in to perform this action."
  }
        }
      },
      "delete": {
        "tags": ["Your Pokemon Cards"],
        "summary": "Delete one of your Pokémon card",
        "responses": {
          "200": { "description": "Card deleted" },
            "401": {
    "description": "You must be logged in to perform this action."
  }
        }
      }
    },
    "/masterSets": {
      "get": {
        "tags": ["Master Sets"],
        "summary": "Get all master sets",
        "responses": {
          "200": { "description": "List of master sets" }
        }
      },
      "post": {
        "tags": ["Master Sets"],
        "summary": "Add a new master set to your collection",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": { "type": "string", "example": "Base Set" },
                  "year": { "type": "integer", "example": 1999 },
                  "cards": { "type": "integer", "minimum": 1, "example": 102 },
                  "percent_completed": { "type": "integer", "minimum": 0, "maximum": 100, "example": 100 }
                },
                "required": ["name", "cards"]
              }
            }
          }
        },
        "responses": {
          "201": { "description": "Master set created" },
          "400": { "description": "Invalid input" },
          "401": {"description": "You must be logged in to perform this action."}
        }
      }
    },
    "/masterSets/{id}": {
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "description": "ID of the master set",
          "schema": { "type": "string" }
        }
      ],
      "get": {
        "tags": ["Master Sets"],
        "summary": "Get a master set price by ID",
        "responses": {
          "200": { "description": "Master set found" },
          "404": { "description": "Master set not found" },
          "401": {"description": "You must be logged in to perform this action."}
        }
      },
      "put": {
        "tags": ["Master Sets"],
        "summary": "Update a master set price",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": { "type": "string", "example": "Base Set" },
                  "year": { "type": "integer", "example": 1999 },
                  "cards": { "type": "integer", "minimum": 1, "example": 102 },
                  "percent_completed": { "type": "integer", "minimum": 0, "maximum": 100, "example": 100 }
                },
                "required": ["name", "cards"]
              }
            }
          }
        },
        "responses": {
          "200": { "description": "Master set updated" },
          "401": {"description": "You must be logged in to perform this action."}
        }
      },
      "delete": {
        "tags": ["Master Sets"],
        "summary": "Delete a master set price",
        "responses": {
          "200": { "description": "Master set deleted" },
          "401": {"description": "You must be logged in to perform this action."}
        }
      }
    },

    "/chase": {
      "get": {
        "tags": ["Chase Cards"],
        "summary": "Get all dream/chase cards",
        "responses": {
          "200": { "description": "List of chase cards" }
        }
      },
      "post": {
        "tags": ["Chase Cards"],
        "summary": "Add a new chase card",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": { "type": "string", "example": "Base Set 1st Edition Holo Charizard (Shadowless)" },
                  "set": { "type": "string", "example": "Base Set" },
                  "psa10_price_usd": { "type": "number", "example": 420000 },
                  "year": { "type": "integer", "example": 1999 }
                },
                "required": ["name", "set", "psa10_price_usd", "year"]
              }
            }
          }
        },
        "responses": {
          "201": { "description": "Chase card created" },
          "400": { "description": "Invalid input" },
          "401": {"description": "You must be logged in to perform this action."}
        }
      }
    },
    "/chase/{id}": {
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "description": "ID of the chase card",
          "schema": { "type": "string" }
        }
      ],
      "get": {
        "tags": ["Chase Cards"],
        "summary": "Get a chase card by ID",
        "responses": {
          "200": { "description": "Chase card found" },
          "404": { "description": "Chase card not found" }
        }
      },
      "put": {
        "tags": ["Chase Cards"],
        "summary": "Update a chase card",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": { "type": "string" },
                  "set": { "type": "string" },
                  "psa10_price_usd": { "type": "number" },
                  "year": { "type": "integer" }
                },
                "required": ["name", "set", "psa10_price_usd", "year"]
              }
            }
          }
        },
        "responses": {
          "200": { "description": "Chase card updated" },
          "400": { "description": "Invalid input" },
          "401": {"description": "You must be logged in to perform this action."}
        }
      },
      "delete": {
        "tags": ["Chase Cards"],
        "summary": "Delete a chase card",
        "responses": {
          "200": { "description": "Chase card deleted" },
          "401": {"description": "You must be logged in to perform this action."}
        }
      }
    },

    "/etb": {
      "get": {
        "tags": ["ETB"],
        "summary": "Get all your Elite Trainer Box",
        "responses": {
          "200": { "description": "List of your ETBs" }
        }
      },
      "post": {
        "tags": ["ETB"],
        "summary": "Add a new Elite Trainer Box price",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": { "type": "string", "example": "151 Elite Trainer Box" },
                  "year": { "type": "integer", "example": 2021 },
                  "price_usd": { "type": "number", "example": 302.3 }
                },
                "required": ["name", "year", "price_usd"]
              }
            }
          }
        },
        "responses": {
          "201": { "description": "ETB added to your collection" },
          "400": { "description": "Invalid input" },
          "401": {"description": "You must be logged in to perform this action."}
        }
      }
    },
    "/etb/{id}": {
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "description": "ID of the Elite Trainer Box ",
          "schema": { "type": "string" }
        }
      ],
      "get": {
        "tags": ["ETB"],
        "summary": "Get an ETB by ID",
        "responses": {
          "200": { "description": "ETB price found" },
          "404": { "description": "ETB price not found" }
        }
      },
      "put": {
        "tags": ["ETB"],
        "summary": "Update an ETB ",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": { "type": "string" },
                  "year": { "type": "integer" },
                  "price_usd": { "type": "number" }
                },
                "required": ["name", "year", "price_usd"]
              }
            }
          }
        },
        "responses": {
          "200": { "description": "ETB price updated" },
          "400": { "description": "Invalid input" },
          "401": {"description": "You must be logged in to perform this action."}
        }
      },
      "delete": {
        "tags": ["ETB"],
        "summary": "Delete an ETB price",
        "responses": {
          "200": { "description": "ETB price deleted" },
          "401": {"description": "You must be logged in to perform this action."}
        }
      }
    }
  }
}
