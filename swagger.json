{
  "openapi": "3.0.0",
  "info": {
    "title": "Pokémon Card Collection API",
    "version": "1.0.0",
    "description": "API for managing Pokémon cards and master set prices."
  },
  "tags": [
    {
      "name": "Your Pokemon Cards",
      "description": "Manage your personal Pokémon card collection."
    },
    {
      "name": "Master Sets",
      "description": "Manage and view master set price data."
    }
  ],
  "paths": {
    "/cards": {
      "get": {
        "tags": ["Your Pokemon Cards"],
        "summary": "Get all of your Pokémon cards",
        "responses": {
          "200": {
            "description": "List of your Pokémon cards"
          }
        }
      },
      "post": {
        "tags": ["Your Pokemon Cards"],
        "summary": "Add a new Pokémon card to your collection",
        "responses": {
          "201": {
            "description": "Card created successfully"
          }
        }
      }
    },
    "/cards/{id}": {
      "get": {
        "tags": ["Your Pokemon Cards"],
        "summary": "Get a Pokémon card by ID",
        "responses": {
          "200": {
            "description": "Pokémon card found"
          },
          "404": {
            "description": "Card not found"
          }
        }
      },
      "put": {
        "tags": ["Your Pokemon Cards"],
        "summary": "Update one of your Pokémon card",
        "responses": {
          "200": {
            "description": "Card updated"
          }
        }
      },
      "delete": {
        "tags": ["Your Pokemon Cards"],
        "summary": "Delete one of your Pokémon card",
        "responses": {
          "200": {
            "description": "Card deleted"
          }
        }
      }
    },
    "/masterSets": {
      "get": {
        "tags": ["Master Sets"],
        "summary": "Get all master set prices",
        "responses": {
          "200": {
            "description": "List of master sets"
          }
        }
      },
      "post": {
        "tags": ["Master Sets"],
        "summary": "Add a new master set price",
        "responses": {
          "201": {
            "description": "Master set created"
          }
        }
      }
    },
    "/masterSets/{id}": {
      "get": {
        "tags": ["Master Sets"],
        "summary": "Get a master set price by ID",
        "responses": {
          "200": {
            "description": "Master set found"
          },
          "404": {
            "description": "Master set not found"
          }
        }
      },
      "put": {
        "tags": ["Master Sets"],
        "summary": "Update a master set price",
        "responses": {
          "200": {
            "description": "Master set updated"
          }
        }
      },
      "delete": {
        "tags": ["Master Sets"],
        "summary": "Delete a master set price",
        "responses": {
          "200": {
            "description": "Master set deleted"
          }
        }
      }
    }
  }
}
